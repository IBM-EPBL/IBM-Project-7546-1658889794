        "        \"tt = ()  # initialization of empty tuple\\n\",\n",
        "        \"t1 = (66,)  # initialization of tuple with a single value\\n\",\n",
        "        \"t = (5, 6)       # Create a tuple\\n\",\n",
        "        \"tt = tt+t1+t\\n\",\n",
        "        \"print(\\\"tt=\\\",tt)\\n\",\n",
        "        \"print(\\\"tt[2]=\\\",tt[2])\\n\",\n",
        "        \"print(\\\"tt[1:3]=\\\",tt[1:3])\\n\",\n",
