        "      \"source\": [\n",
        "        \"def quicksort(arr):\\n\",\n",
        "        \"    if len(arr) <= 1:\\n\",\n",
        "        \"        return arr\\n\",\n",
        "        \"    pivot = arr[len(arr) // 2]\\n\",\n",
        "        \"    left = [x for x in arr if x < pivot]\\n\",\n",
        "        \"    middle = [x for x in arr if x == pivot]\\n\",\n",
        "        \"    right = [x for x in arr if x > pivot]\\n\",\n",
        "        \"    return quicksort(left) + middle + quicksort(right)\\n\",\n",
        "        \"\\n\",\n",
        "        \"print(quicksort([3,6,8,10,1,2,1]))\"\n",
        "      ],\n",
