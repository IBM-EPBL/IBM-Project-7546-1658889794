        "            \"text/plain\": [\n",
        "              \"'papa'\"\n",
        "            ]\n",
        "          },\n",
        "          \"metadata\": {\n",
        "            \"tags\": []\n",
        "          },\n",
        "          \"execution_count\": 50\n",
        "        }\n",
        "      ]\n",
        "    },\n",
        "    {\n",
        "      \"cell_type\": \"markdown\",\n",
        "      \"metadata\": {\n",
        "        \"id\": \"ejkev803U6ch\"\n",
        "      },\n",
        "      \"source\": [\n",
