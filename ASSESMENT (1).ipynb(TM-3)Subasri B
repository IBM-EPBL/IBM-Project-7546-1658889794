        "            \"[[False False]\\n\",\n",
        "            \" [ True  True]\\n\",\n",
        "            \" [ True  True]]\\n\"\n",
        "          ],\n",
        "          \"name\": \"stdout\"\n",
        "        }\n",
        "      ]\n",
        "    },\n",
        "    {\n",
        "      \"cell_type\": \"code\",\n",
        "      \"metadata\": {\n",
